{
	"env": {
		"node": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"indent": [2, "tab",
			{"SwitchCase": 1}
		],
		"quotes": 0,
		"no-console": 0,
		"semi": [
			"error",
			"always"
		],
		"no-process-exit": 0,
		"no-loop-func": 0,
		"no-multi-spaces": 0,
		"no-underscore-dangle": 0,
		"camelcase": 0,
		"curly": 0,
		"no-use-before-define": 0,
		"key-spacing": 0,
		"space-infix-ops": 0,
		"space-unary-ops": 0,
		"no-unused-vars": [2, {
			"vars": "all",
			"args": "none"
		}]
    }
}
