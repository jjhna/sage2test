{
	name: "SAGE2 test",
	host: "sage2server.evl.uic.edu",
	port: 443,
	index_port: 80,
	background: {
		color: "#333333",
		watermark: {
			svg: "images/sage2.svg",
			color: "rgba(255, 255, 255, 0.5)"
		}
	},
	security: {
		secureProtocol: "TLSv1_2_method"
