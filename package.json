{
	"name": "SAGE2",
	"version": "0.3.0",
	"private": true,
	"description": "A cluster-based html viewer used for displaying elements across multiple browser windows",
	"keywords": [
		"SAGE",
		"browser",
		"collaboration"
	],
	"author": "UIC EVL <evl.avatar@gmail.com>",
	"contributors": [
