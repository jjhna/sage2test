	},
	"scripts": {
		"start":   "node server.js -l",
		"monitor": "./node_modules/.bin/nodemon -e json,js,html -I -L -i public/uploads server.js -l",
		"lint":    "./node_modules/.bin/grunt eslint",
		"style":   "./node_modules/.bin/grunt jscs",
		"doc":     "./node_modules/.bin/grunt yuidoc",
		"mini":    "./node_modules/.bin/grunt uglify --verbose",
		"newapp":  "./node_modules/.bin/grunt newapp",
		"test":    "./node_modules/.bin/grunt all",
		"in":      "node ./install_dependencies.js",
		"inprod":  "node ./install_dependencies.js --prod"
